class Solution {
    public int lengthOfLongestSubstring(String s) {
        HashSet<Character> set=new HashSet<>();
        int i=0;
        int j=0;
        int ans=0;
        while(j<s.length() && i<s.length()){
           char c=s.charAt(j);
           while(set.contains(c)){
                set.remove(s.charAt(i));
                i++;
           }
           set.add(c);
           ans=Math.max(ans,j-i+1);
           j++;
        }
        return ans;
    }
}
